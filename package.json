{
  "name": "pure-html",
  "version": "0.3.3",
  "description": "A dev environment for creating standalone html files",
  "main": "bin/pure-html",
  "bin": {
    "pure-html": "./bin/pure-html"
  },
  "scripts": {
    "dev": "node bin/pure-html -d -s",
    "build": "node bin/pure-html -s",
    "test": "node test/test.js",
    "precommit": "prettier --single-quote --bracket-spacing true --trailing-comma none --write \"lib/**/*.js\" && npm test",
    "prepush": "npm test",
    "prepublish": "pkg-ok"
  },
  "author": "Samir Alajmovic <alajmovic.samir@gmail.com> (https://www.alajmovic.com)",
  "license": "MIT",
  "devDependencies": {
    "husky": "^0.13.4",
    "pkg-ok": "^1.1.0",
    "prettier": "^1.5.2",
    "tap": "^10.7.0"
  },
  "dependencies": {
    "browser-sync": "^2.18.12",
    "bs-html-injector": "^3.0.3",
    "commander": "^2.11.0",
    "del": "^2.2.2",
    "glob": "^7.1.2",
    "gulp": "github:gulpjs/gulp#4.0",
    "html5-lint": "^0.3.0",
    "inline-source": "^5.2.3"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/samiralajmovic/pure-html.git"
  },
  "homepage": "https://github.com/samiralajmovic/pure-html",
  "keywords": [
    "html"
  ]
}
